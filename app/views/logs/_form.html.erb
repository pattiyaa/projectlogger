<%= semantic_form_for @log do |f| %>
  <%= f.inputs do %>
    <%= f.input :logdate, :as => :datetime_picker  %>
    <%= f.input :data , :as => :ckeditor ,input_html: {:ckeditor => {:toolbar => 'mini'}}%>
    <%= f.input :logperson %>
    <%= f.input :project %>
    <%= f.input  :pictures, :as => :filesupload,:image_preview=> true %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
